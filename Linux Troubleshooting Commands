arch

cat /etc*-release

echo $MACHTYPE

uname -ar

uptime -p

free -m / -g / -h

who

lsof -i

cat /etc/passwd

cat /proc/cpuinfo


cat /proc/meminfo

df -h

du -sh

ps aux

netstat -antlup

info command

man -k command

rpm -ql httpd

top






